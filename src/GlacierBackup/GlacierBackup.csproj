<Project Sdk="Microsoft.NET.Sdk">

    <PropertyGroup>
        <Description>Tool to backup assets to Amazon Glacier</Description>
        <AssemblyTitle>GlacierBackup</AssemblyTitle>
        <VersionPrefix>0.2.0</VersionPrefix>
        <Authors>Mike Morano &lt;mmorano@mikeandwan.us&gt;</Authors>
        <TargetFramework>net5.0</TargetFramework>
        <TreatWarningsAsErrors>true</TreatWarningsAsErrors>
        <DebugType>portable</DebugType>
        <AssemblyName>GlacierBackup</AssemblyName>
        <OutputType>Exe</OutputType>
        <PackageId>GlacierBackup</PackageId>
        <PackageProjectUrl>https://github.com/AerisG222/GlacierBackup</PackageProjectUrl>
        <PackageLicenseUrl>https://github.com/AerisG222/GlacierBackup/blob/master/LICENSE.md</PackageLicenseUrl>
    </PropertyGroup>

    <ItemGroup>
        <PackageReference Include="AWSSDK.Glacier" Version="3.5.0.45" />
        <PackageReference Include="CsvHelper" Version="17.0.1" />
    </ItemGroup>

</Project>
