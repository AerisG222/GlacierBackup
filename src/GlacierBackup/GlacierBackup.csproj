<Project Sdk="Microsoft.NET.Sdk">

  <PropertyGroup>
    <Description>Tool to backup assets to Amazon Glacier</Description>
    <AssemblyTitle>GlacierBackup</AssemblyTitle>
    <VersionPrefix>0.1.0</VersionPrefix>
    <Authors>Mike Morano &lt;mmorano@mikeandwan.us&gt;</Authors>
    <TargetFramework>netcoreapp1.1</TargetFramework>
    <TreatWarningsAsErrors>true</TreatWarningsAsErrors>
    <DebugType>portable</DebugType>
    <AssemblyName>GlacierBackup</AssemblyName>
    <OutputType>Exe</OutputType>
    <PackageId>GlacierBackup</PackageId>
    <PackageProjectUrl>https://github.com/AerisG222/GlacierBackup</PackageProjectUrl>
    <PackageLicenseUrl>https://github.com/AerisG222/GlacierBackup/blob/master/LICENSE.md</PackageLicenseUrl>
    <RuntimeFrameworkVersion>1.1.1</RuntimeFrameworkVersion>
  </PropertyGroup>

  <ItemGroup>
    <PackageReference Include="AWSSDK.Glacier" Version="3.3.1.3" />
    <PackageReference Include="CsvHelper" Version="2.16.3" />
  </ItemGroup>

</Project>
